{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "plugins": ["react"],
  "rules": {
    "semi": "off",
    "object-curly-newline": "off",
    "no-use-before-define": "warn",

    "react/jsx-filename-extension": "off",
    "react/jsx-one-expression-per-line": "off",
    "react/prop-types": "off"
  }
}
